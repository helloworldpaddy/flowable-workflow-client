<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
             xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
             xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
             xmlns:flowable="http://flowable.org/bpmn"
             xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             id="Definitions_CMS_Process"
             targetNamespace="http://www.flowable.org/processdef"
             exporter="Flowable"
             exporterVersion="7.0.0.M1">

  <!-- Main CMS Process -->
  <process id="Process_CMS_Workflow_Updated" name="CMS Workflow Process Updated" isExecutable="true">

    <startEvent id="StartEvent_ComplaintReceived" name="Complaint Received">
      <outgoing>Flow_to_Intake</outgoing>
    </startEvent>

    <!-- 1. Intake and Assignment -->
    <userTask id="Task_EO_Intake" name="EO Intake - Initial Review">
      <extensionElements>
        <flowable:candidateGroups>INTAKE_ANALYST_GROUP</flowable:candidateGroups>
        <flowable:formKey>eoIntakeForm</flowable:formKey>
      </extensionElements>
      <incoming>Flow_to_Intake</incoming>
      <outgoing>Flow_to_DetermineRouting</outgoing>
    </userTask>

    <!-- Service Task to Call DMN per Allegation and Aggregate Classifications -->
    <!-- This task will take the 'allegations' list, call the DMN for each,
         and populate 'relevantDepartments' (e.g., ["HR", "LEGAL"]) and 'casePriority' -->
    <serviceTask id="ServiceTask_DetermineDepartmentRouting" name="Determine Department Routing"
                 flowable:class="com.workflow.cmsflowable.service.DetermineDepartmentRoutingService">
      <!-- In a real implementation, this service would fetch allegations from process variables,
           iterate, call the DMN (e.g., using DmnRuleService), and set relevantDepartments and casePriority. -->
      <incoming>Flow_to_DetermineRouting</incoming>
      <outgoing>Flow_to_Gateway</outgoing>
    </serviceTask>

    <exclusiveGateway id="ExclusiveGateway_RouteByDepartments" name="Route by Departments">
      <incoming>Flow_to_Gateway</incoming>
      <outgoing>Flow_to_SingleDepartment</outgoing>
      <outgoing>Flow_to_MultiDepartment</outgoing>
    </exclusiveGateway>

    <!-- Path for single department assignment -->
    <userTask id="Task_CreateWorkItem" name="Create Work Item (Single Dept)">
      <extensionElements>
        <flowable:candidateGroups>WORK_ITEM_GROUP</flowable:candidateGroups>
        <flowable:formKey>workItemForm</flowable:formKey>
      </extensionElements>
      <incoming>Flow_to_SingleDepartment</incoming>
      <outgoing>Flow_SingleDept_to_IUIntake</outgoing>
    </userTask>

    <!-- Path for multi-department assignment (Multi-Instance Subprocess) -->
    <subProcess id="SubProcess_DepartmentAssignments" name="Department Assignments">
      <multiInstanceLoopCharacteristics isSequential="false" flowable:collection="relevantDepartments" flowable:elementVariable="department">
        <loopCardinality xsi:type="tFormalExpression">${relevantDepartments.size()}</loopCardinality>
      </multiInstanceLoopCharacteristics>

      <startEvent id="StartEvent_DeptAssignment" name="Start Dept Assignment">
        <outgoing>Flow_StartDept_to_Task</outgoing>
      </startEvent>

      <userTask id="Task_AssignToDepartment" name="Assign to ${department}">
        <extensionElements>
          <!-- Dynamically assign to the group based on the 'department' variable -->
          <flowable:candidateGroups>${department}_GROUP</flowable:candidateGroups>
          <flowable:formKey>departmentAssignmentForm</flowable:formKey>
        </extensionElements>
        <incoming>Flow_StartDept_to_Task</incoming>
        <outgoing>Flow_Task_to_EndDept</outgoing>
      </userTask>

      <endEvent id="EndEvent_DeptAssignment" name="End Dept Assignment">
        <incoming>Flow_Task_to_EndDept</incoming>
      </endEvent>

      <sequenceFlow id="Flow_StartDept_to_Task" sourceRef="StartEvent_DeptAssignment" targetRef="Task_AssignToDepartment" />
      <sequenceFlow id="Flow_Task_to_EndDept" sourceRef="Task_AssignToDepartment" targetRef="EndEvent_DeptAssignment" />
    </subProcess>

    <!-- 2. IU Intake and Investigation Planning -->
    <userTask id="Task_IU_IntakeReview" name="IU Intake Review">
      <extensionElements>
        <flowable:candidateGroups>IU_ANALYST_GROUP,IU_MANAGER_GROUP</flowable:candidateGroups>
        <flowable:formKey>iuIntakeReviewForm</flowable:formKey>
      </extensionElements>
      <incoming>Flow_SingleDept_to_IUIntake</incoming>
      <incoming>Flow_MultiDept_to_IUIntake</incoming>
      <outgoing>Flow_to_DraftIP</outgoing>
    </userTask>

    <userTask id="Task_DraftInvestigationPlan" name="Draft Investigation Plan (IP)">
      <extensionElements>
        <flowable:candidateGroups>INVESTIGATOR_GROUP</flowable:candidateGroups>
        <flowable:formKey>draftIPForm</flowable:formKey>
      </extensionElements>
      <incoming>Flow_to_DraftIP</incoming>
      <incoming>Flow_IP_Feedback</incoming>
      <outgoing>Flow_to_IPApproval</outgoing>
    </userTask>

    <exclusiveGateway id="ExclusiveGateway_IP_Approved" name="IP Approved?">
      <incoming>Flow_to_IPApproval</incoming>
      <outgoing>Flow_IP_Approved</outgoing>
      <outgoing>Flow_IP_Feedback</outgoing>
    </exclusiveGateway>

    <!-- 3. Active Investigation -->
    <userTask id="Task_ConductInvestigation" name="Conduct Investigation">
      <extensionElements>
        <flowable:candidateGroups>INVESTIGATOR_GROUP</flowable:candidateGroups>
        <flowable:formKey>conductInvestigationForm</flowable:formKey>
      </extensionElements>
      <incoming>Flow_IP_Approved</incoming>
      <outgoing>Flow_to_DraftROI</outgoing>
    </userTask>

    <userTask id="Task_DraftROI" name="Draft Report of Investigation (ROI)">
      <extensionElements>
        <flowable:candidateGroups>INVESTIGATOR_GROUP</flowable:candidateGroups>
        <flowable:formKey>draftROIForm</flowable:formKey>
      </extensionElements>
      <incoming>Flow_to_DraftROI</incoming>
      <incoming>Flow_ROI_Feedback</incoming>
      <outgoing>Flow_to_ROIApproval</outgoing>
    </userTask>

    <exclusiveGateway id="ExclusiveGateway_ROI_Approved" name="ROI Approved?">
      <incoming>Flow_to_ROIApproval</incoming>
      <outgoing>Flow_ROI_Approved</outgoing>
      <outgoing>Flow_ROI_Feedback</outgoing>
    </exclusiveGateway>

    <!-- 4. Finalization and Discipline -->
    <userTask id="Task_AssessFindings" name="Assess Findings &amp; Substantiation">
      <extensionElements>
        <flowable:candidateGroups>INVESTIGATION_MANAGER_GROUP</flowable:candidateGroups>
        <flowable:formKey>assessFindingsForm</flowable:formKey>
      </extensionElements>
      <incoming>Flow_ROI_Approved</incoming>
      <outgoing>Flow_to_SubstantiationGateway</outgoing>
    </userTask>

    <exclusiveGateway id="ExclusiveGateway_AllegationsSubstantiated" name="Allegations Substantiated?">
      <incoming>Flow_to_SubstantiationGateway</incoming>
      <outgoing>Flow_Substantiated_Yes</outgoing>
      <outgoing>Flow_Substantiated_No</outgoing>
    </exclusiveGateway>

    <userTask id="Task_HR_LegalAdjudication" name="HR Legal Adjudication">
      <extensionElements>
        <flowable:candidateGroups>HR_LEGAL_ADJUDICATION_GROUP</flowable:candidateGroups>
        <flowable:formKey>hrLegalAdjudicationForm</flowable:formKey>
      </extensionElements>
      <incoming>Flow_Substantiated_Yes</incoming>
      <outgoing>Flow_to_CoordinateDiscipline</outgoing>
    </userTask>

    <userTask id="Task_CoordinateDiscipline" name="Coordinate Disciplinary Action">
      <extensionElements>
        <flowable:candidateGroups>HR_GROUP</flowable:candidateGroups>
        <flowable:formKey>coordinateDisciplineForm</flowable:formKey>
      </extensionElements>
      <incoming>Flow_to_CoordinateDiscipline</incoming>
      <outgoing>Flow_AfterDiscipline</outgoing>
    </userTask>

    <!-- 5. AROG Review (Audit or Oversight Group) -->
    <exclusiveGateway id="ExclusiveGateway_AROG_CriteriaMet" name="AROG Criteria Met?">
      <incoming>Flow_AfterSubstantiationDecision</incoming>
      <incoming>Flow_AfterDiscipline</incoming>
      <outgoing>Flow_AROG_Yes</outgoing>
      <outgoing>Flow_AROG_No</outgoing>
    </exclusiveGateway>

    <userTask id="Task_AROG_ReviewMeeting" name="AROG Review Meeting">
      <extensionElements>
        <flowable:candidateGroups>AROG_GROUP</flowable:candidateGroups>
        <flowable:formKey>arogReviewForm</flowable:formKey>
      </extensionElements>
      <incoming>Flow_AROG_Yes</incoming>
      <outgoing>Flow_AfterAROG</outgoing>
    </userTask>

    <!-- 6. EO Final Review and Closure -->
    <userTask id="Task_EO_FinalReviewAndClosure" name="EO Final Review and Closure">
      <extensionElements>
        <flowable:candidateGroups>DIRECTOR_GROUP</flowable:candidateGroups>
        <flowable:formKey>finalReviewClosureForm</flowable:formKey>
      </extensionElements>
      <incoming>Flow_AfterAROG</incoming>
      <incoming>Flow_AROG_No</incoming>
      <incoming>Flow_Closure_Feedback</incoming>
      <outgoing>Flow_to_ClosureApproval</outgoing>
    </userTask>

    <exclusiveGateway id="ExclusiveGateway_CaseApprovedForClosure" name="Case Approved for Closure?">
      <incoming>Flow_to_ClosureApproval</incoming>
      <outgoing>Flow_Closure_Approved</outgoing>
      <outgoing>Flow_Closure_Feedback</outgoing>
    </exclusiveGateway>

    <endEvent id="EndEvent_CaseClosed" name="Case Closed">
      <incoming>Flow_Closure_Approved</incoming>
    </endEvent>

    <!-- Main Process Sequence Flows -->
    <sequenceFlow id="Flow_to_Intake" sourceRef="StartEvent_ComplaintReceived" targetRef="Task_EO_Intake" />
    <sequenceFlow id="Flow_to_DetermineRouting" sourceRef="Task_EO_Intake" targetRef="ServiceTask_DetermineDepartmentRouting" />
    <sequenceFlow id="Flow_to_Gateway" sourceRef="ServiceTask_DetermineDepartmentRouting" targetRef="ExclusiveGateway_RouteByDepartments" />

    <!-- Condition Expressions - Converted from FEEL to JUEL -->
    <!-- Assuming 'relevantDepartments' is a List of Strings (e.g., ["HR", "LEGAL"]) -->
    <sequenceFlow id="Flow_to_SingleDepartment" name="Single Department" sourceRef="ExclusiveGateway_RouteByDepartments" targetRef="Task_CreateWorkItem">
      <conditionExpression xsi:type="tFormalExpression">${relevantDepartments.size() == 1}</conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="Flow_to_MultiDepartment" name="Multi-Department" sourceRef="ExclusiveGateway_RouteByDepartments" targetRef="SubProcess_DepartmentAssignments">
      <conditionExpression xsi:type="tFormalExpression">${relevantDepartments.size() > 1}</conditionExpression>
    </sequenceFlow>

    <!-- Convergence points -->
    <sequenceFlow id="Flow_SingleDept_to_IUIntake" sourceRef="Task_CreateWorkItem" targetRef="Task_IU_IntakeReview" />
    <sequenceFlow id="Flow_MultiDept_to_IUIntake" sourceRef="SubProcess_DepartmentAssignments" targetRef="Task_IU_IntakeReview" />

    <sequenceFlow id="Flow_to_DraftIP" sourceRef="Task_IU_IntakeReview" targetRef="Task_DraftInvestigationPlan" />
    <sequenceFlow id="Flow_to_IPApproval" sourceRef="Task_DraftInvestigationPlan" targetRef="ExclusiveGateway_IP_Approved" />
    <sequenceFlow id="Flow_IP_Approved" name="Yes" sourceRef="ExclusiveGateway_IP_Approved" targetRef="Task_ConductInvestigation">
      <conditionExpression xsi:type="tFormalExpression">${ipApproved == true}</conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="Flow_IP_Feedback" name="No / Feedback" sourceRef="ExclusiveGateway_IP_Approved" targetRef="Task_DraftInvestigationPlan">
      <conditionExpression xsi:type="tFormalExpression">${ipApproved == false}</conditionExpression>
    </sequenceFlow>

    <sequenceFlow id="Flow_to_DraftROI" sourceRef="Task_ConductInvestigation" targetRef="Task_DraftROI" />
    <sequenceFlow id="Flow_to_ROIApproval" sourceRef="Task_DraftROI" targetRef="ExclusiveGateway_ROI_Approved" />
    <sequenceFlow id="Flow_ROI_Approved" name="Yes" sourceRef="ExclusiveGateway_ROI_Approved" targetRef="Task_AssessFindings">
      <conditionExpression xsi:type="tFormalExpression">${roiApproved == true}</conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="Flow_ROI_Feedback" name="No / Feedback" sourceRef="ExclusiveGateway_ROI_Approved" targetRef="Task_DraftROI">
      <conditionExpression xsi:type="tFormalExpression">${roiApproved == false}</conditionExpression>
    </sequenceFlow>

    <sequenceFlow id="Flow_to_SubstantiationGateway" sourceRef="Task_AssessFindings" targetRef="ExclusiveGateway_AllegationsSubstantiated" />
    <sequenceFlow id="Flow_Substantiated_Yes" name="Yes" sourceRef="ExclusiveGateway_AllegationsSubstantiated" targetRef="Task_HR_LegalAdjudication">
      <conditionExpression xsi:type="tFormalExpression">${allegationsSubstantiated == true}</conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="Flow_Substantiated_No" name="No" sourceRef="ExclusiveGateway_AllegationsSubstantiated" targetRef="ExclusiveGateway_AROG_CriteriaMet">
      <conditionExpression xsi:type="tFormalExpression">${allegationsSubstantiated == false}</conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="Flow_to_CoordinateDiscipline" sourceRef="Task_HR_LegalAdjudication" targetRef="Task_CoordinateDiscipline" />
    <sequenceFlow id="Flow_AfterDiscipline" sourceRef="Task_CoordinateDiscipline" targetRef="ExclusiveGateway_AROG_CriteriaMet" />
    <sequenceFlow id="Flow_AfterSubstantiationDecision" sourceRef="ExclusiveGateway_AllegationsSubstantiated" targetRef="ExclusiveGateway_AROG_CriteriaMet" />

    <sequenceFlow id="Flow_AROG_Yes" name="Yes" sourceRef="ExclusiveGateway_AROG_CriteriaMet" targetRef="Task_AROG_ReviewMeeting">
      <conditionExpression xsi:type="tFormalExpression">${isArogCase == true}</conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="Flow_AROG_No" name="No" sourceRef="ExclusiveGateway_AROG_CriteriaMet" targetRef="Task_EO_FinalReviewAndClosure">
      <conditionExpression xsi:type="tFormalExpression">${isArogCase == false}</conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="Flow_AfterAROG" sourceRef="Task_AROG_ReviewMeeting" targetRef="Task_EO_FinalReviewAndClosure" />

    <sequenceFlow id="Flow_to_ClosureApproval" sourceRef="Task_EO_FinalReviewAndClosure" targetRef="ExclusiveGateway_CaseApprovedForClosure" />
    <sequenceFlow id="Flow_Closure_Approved" name="Yes" sourceRef="ExclusiveGateway_CaseApprovedForClosure" targetRef="EndEvent_CaseClosed">
      <conditionExpression xsi:type="tFormalExpression">${caseApprovedForClosure == true}</conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="Flow_Closure_Feedback" name="No / Feedback" sourceRef="ExclusiveGateway_CaseApprovedForClosure" targetRef="Task_EO_FinalReviewAndClosure">
      <conditionExpression xsi:type="tFormalExpression">${caseApprovedForClosure == false}</conditionExpression>
    </sequenceFlow>

  </process>

  <!-- BPMN Diagram Interchange (BPMNDI) -->
  <bpmndi:BPMNDiagram id="BPMNDiagram_CMS_Process">
    <bpmndi:BPMNPlane id="BPMNPlane_CMS_Process" bpmnElement="Process_CMS_Workflow_Updated">

      <!-- Start Event -->
      <bpmndi:BPMNShape id="StartEvent_ComplaintReceived_di" bpmnElement="StartEvent_ComplaintReceived">
        <dc:Bounds x="152" y="102" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="136" y="145" width="68" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- EO Intake Task -->
      <bpmndi:BPMNShape id="Task_EO_Intake_di" bpmnElement="Task_EO_Intake">
        <dc:Bounds x="240" y="80" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <!-- Determine Department Routing Service Task -->
      <bpmndi:BPMNShape id="ServiceTask_DetermineDepartmentRouting_di" bpmnElement="ServiceTask_DetermineDepartmentRouting">
        <dc:Bounds x="390" y="80" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <!-- Route by Departments Gateway -->
      <bpmndi:BPMNShape id="ExclusiveGateway_RouteByDepartments_di" bpmnElement="ExclusiveGateway_RouteByDepartments" isMarkerVisible="true">
        <dc:Bounds x="545" y="95" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="528" y="65" width="84" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- Create Work Item Task (Single Dept) -->
      <bpmndi:BPMNShape id="Task_CreateWorkItem_di" bpmnElement="Task_CreateWorkItem">
        <dc:Bounds x="650" y="180" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <!-- Department Assignments Subprocess (Multi-Instance) -->
      <bpmndi:BPMNShape id="SubProcess_DepartmentAssignments_di" bpmnElement="SubProcess_DepartmentAssignments" isExpanded="true">
        <dc:Bounds x="620" y="320" width="400" height="180" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <!-- Subprocess elements -->
      <bpmndi:BPMNShape id="StartEvent_DeptAssignment_di" bpmnElement="StartEvent_DeptAssignment">
        <dc:Bounds x="652" y="392" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="632" y="435" width="76" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_AssignToDepartment_di" bpmnElement="Task_AssignToDepartment">
        <dc:Bounds x="760" y="370" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="EndEvent_DeptAssignment_di" bpmnElement="EndEvent_DeptAssignment">
        <dc:Bounds x="922" y="392" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="897" y="435" width="86" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- IU Intake Review Task -->
      <bpmndi:BPMNShape id="Task_IU_IntakeReview_di" bpmnElement="Task_IU_IntakeReview">
        <dc:Bounds x="1100" y="220" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <!-- Draft Investigation Plan Task -->
      <bpmndi:BPMNShape id="Task_DraftInvestigationPlan_di" bpmnElement="Task_DraftInvestigationPlan">
        <dc:Bounds x="1250" y="220" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <!-- IP Approved Gateway -->
      <bpmndi:BPMNShape id="ExclusiveGateway_IP_Approved_di" bpmnElement="ExclusiveGateway_IP_Approved" isMarkerVisible="true">
        <dc:Bounds x="1405" y="235" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1394" y="211" width="67" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- Conduct Investigation Task -->
      <bpmndi:BPMNShape id="Task_ConductInvestigation_di" bpmnElement="Task_ConductInvestigation">
        <dc:Bounds x="1500" y="220" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <!-- Draft ROI Task -->
      <bpmndi:BPMNShape id="Task_DraftROI_di" bpmnElement="Task_DraftROI">
        <dc:Bounds x="1650" y="220" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <!-- ROI Approved Gateway -->
      <bpmndi:BPMNShape id="ExclusiveGateway_ROI_Approved_di" bpmnElement="ExclusiveGateway_ROI_Approved" isMarkerVisible="true">
        <dc:Bounds x="1805" y="235" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1794" y="211" width="67" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- Assess Findings Task -->
      <bpmndi:BPMNShape id="Task_AssessFindings_di" bpmnElement="Task_AssessFindings">
        <dc:Bounds x="1900" y="220" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <!-- Allegations Substantiated Gateway -->
      <bpmndi:BPMNShape id="ExclusiveGateway_AllegationsSubstantiated_di" bpmnElement="ExclusiveGateway_AllegationsSubstantiated" isMarkerVisible="true">
        <dc:Bounds x="2055" y="235" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="2035" y="205" width="90" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- HR Legal Adjudication Task -->
      <bpmndi:BPMNShape id="Task_HR_LegalAdjudication_di" bpmnElement="Task_HR_LegalAdjudication">
        <dc:Bounds x="2170" y="180" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <!-- Coordinate Discipline Task -->
      <bpmndi:BPMNShape id="Task_CoordinateDiscipline_di" bpmnElement="Task_CoordinateDiscipline">
        <dc:Bounds x="2320" y="180" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <!-- AROG Criteria Met Gateway -->
      <bpmndi:BPMNShape id="ExclusiveGateway_AROG_CriteriaMet_di" bpmnElement="ExclusiveGateway_AROG_CriteriaMet" isMarkerVisible="true">
        <dc:Bounds x="2485" y="235" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="2464" y="205" width="92" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- AROG Review Meeting Task -->
      <bpmndi:BPMNShape id="Task_AROG_ReviewMeeting_di" bpmnElement="Task_AROG_ReviewMeeting">
        <dc:Bounds x="2600" y="180" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <!-- EO Final Review and Closure Task -->
      <bpmndi:BPMNShape id="Task_EO_FinalReviewAndClosure_di" bpmnElement="Task_EO_FinalReviewAndClosure">
        <dc:Bounds x="2750" y="220" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>

      <!-- Case Approved For Closure Gateway -->
      <bpmndi:BPMNShape id="ExclusiveGateway_CaseApprovedForClosure_di" bpmnElement="ExclusiveGateway_CaseApprovedForClosure" isMarkerVisible="true">
        <dc:Bounds x="2905" y="235" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="2885" y="205" width="90" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- End Event -->
      <bpmndi:BPMNShape id="EndEvent_CaseClosed_di" bpmnElement="EndEvent_CaseClosed">
        <dc:Bounds x="3022" y="242" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="3009" y="285" width="63" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- Main Process Sequence Flows -->
      <bpmndi:BPMNEdge id="Flow_to_Intake_di" bpmnElement="Flow_to_Intake">
        <di:waypoint x="188" y="120" />
        <di:waypoint x="240" y="120" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_to_DetermineRouting_di" bpmnElement="Flow_to_DetermineRouting">
        <di:waypoint x="340" y="120" />
        <di:waypoint x="390" y="120" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_to_Gateway_di" bpmnElement="Flow_to_Gateway">
        <di:waypoint x="490" y="120" />
        <di:waypoint x="545" y="120" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_to_SingleDepartment_di" bpmnElement="Flow_to_SingleDepartment">
        <di:waypoint x="570" y="145" />
        <di:waypoint x="570" y="220" />
        <di:waypoint x="650" y="220" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="546" y="174" width="96" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_to_MultiDepartment_di" bpmnElement="Flow_to_MultiDepartment">
        <di:waypoint x="570" y="145" />
